{
    "manifest_version": 2,
    "name": "LinkedIn Feed Blocker",
    "version": "1.0",
    "description": "Block news feed on LinkedIn",
    "icons": {
        "48": "icons/border-48.png",
        "96": "icons/border-96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.linkedin.com/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ]
}
